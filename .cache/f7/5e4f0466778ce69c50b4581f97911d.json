r(marginals);\r\n        border-radius: 0 0 8px 8px;\r\n    }\r\n    #header h1 {\r\n        padding: 0.5em;\r\n    }\r\n    h1 {\r\n        color: color(heading);\r\n        font-size: 30px;\r\n\r\n    }\r\n}\r\n\r\n/* layout ********/\r\n\r\n\r\n.wrapper {\r\n\tdisplay: flex;\r\n\tmargin: 3em 0;\r\n}\r\n#content {\r\n\twidth: 66%;\r\n}\r\n#sidebar h1 {\r\n\tfont-size: 26px;\r\n}\r\n#sidebar {\r\n\tz-index: 1;\r\n\tbackground-color: color(background);\r\n\topacity: 1;\r\n\twidth: 33%;\r\n\tpadding-left: 1em;\r\n}\r\n#footer {\r\n\tbackground-color: color(marginals);\r\n\tborder-radius: 8px 8px 0 0;\r\n\tposition: absolute;\r\n\tbottom: 0;\r\n\twidth: 100%;\r\n\tmax-width: 940px;\r\n\ttext-align: right;\r\n}\r\n/*slider elements*/\r\n\r\n.slider1,\r\n.slider2 {\r\n\tdisplay: flex;\r\n\tflex-flow: row-reverse nowrap;\r\n\tmax-width: 100%;\r\n\tposition: relative;\r\n\toverflow: visible;\r\n\tmin-height: 200px;\r\n}\r\n.sl1,\r\n.sl2 {\r\n\tdisplay: flex;\r\n\tflex-flow: row-reverse;\r\n\tposition: relative;\r\n\tjustify-content: start;\r\n\toverflow: visible;\r\n}\r\nimg {\r\n\tposition: relative;\r\n}\r\n.slideEl {\r\n\tborder-radius: 5%;\r\n}\r\n.flexEl {\r\n\tmax-height: 200px;\r\n\tdisplay: inline;\r\n\tpadding: 5px;\r\n}\r\n.arrowDiv {\r\n\tpadding-left: 5em;\r\n\tposition: relative;\r\n\tz-index: 1;\r\n    background-color: color(background);\r\n\topacity: 1;\r\n}\r\n.arrow {\r\n\talign-content: flex-end;\r\n\tdisplay: block;\r\n\tpadding: 2.5em;\r\n\tmargin: 1em;\r\n\tborder-radius: 10%;\r\n}\r\n.inactive.left {\r\n\tbackground: url(../img/arrow-gray-left.png) no-repeat center, color(background) no-repeat center;\r\n}\r\n.active.left {\r\n\tbackground: url(../img/arrow-blue-left.png) no-repeat center, color(background) no-repeat center;\r\n}\r\n.active.left:hover {\r\n\tbackground: url(../img/arrow-blue-left.png) no-repeat center, color(inactive) no-repeat center;\r\n}\r\n.active.right {\r\n\tbackground: url(../img/arrow-blue-right.png) no-repeat center, color(background) no-repeat center;\r\n}\r\n.active.right:hover {\r\n\tbackground: url(../img/arrow-blue-right.png) no-repeat center, color(inactive) no-repeat center;\r\n}\r\n.active {\r\n\tborder: 1px solid color(arrow);\r\n\tbox-shadow: 0 2px 2px 0 color(arrow);\r\n}\r\n.inactive {\r\n\tborder: 1px solid color(marginals);\r\n\tbox-shadow: 0 2px 2px 0 #aaa;\r\n}\r\n@media screen and (max-width: 600px) {\r\n\t.wrapper {\r\n\t\tflex-direction: column;\r\n\t}\r\n\t#sidebar,\r\n\t#content {\r\n\t\twidth: 100%;\r\n\t\tmargin: 1em auto;\r\n    }\r\n    #footer {\r\n        position: relative;\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"c0b66b22f1e13f008d6e953eeb830287","cacheData":{"env":{}}}